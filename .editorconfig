root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
indent_style = space
indent_size = 4
trim_trailing_whitespace = true

[*.{cmd,config,csproj,cxxproj,json,props,ps1,resx,sh,targets,yml}]
indent_size = 2

[*.sln]
end_of_line = crlf
indent_style = tab

